{#
/**
 * @file
 * Default theme implementation to present a media entity.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}

{% if content %}
  <div class="col-md-4 col-sm-12">
    <div class="card" {{ attributes }}>
      <div  class="card-img-top">
        <svg class="remove-icon-collection-list button--remove-in-collection remove-icon-collection-list-hidden" version="1.2" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" overflow="visible" preserveAspectRatio="none" viewBox="0 0 24 24" width="22" height="22"><g><path xmlns:default="http://www.w3.org/2000/svg" d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z" style="fill: rgb(18, 18, 18);" vector-effect="non-scaling-stroke"/></g></svg>
        <a class="card-img-overlay" href="/media/{{mid}}"></a>
        {{ content.thumbnail }}
      </div>
      {# <div class="card-body">
        <div class="text-overlay">
          <div class="card--inner-text">
            <p class="title">{{ content.name }}</p>
            <p class="keywords">{{ content.field_keywords }}</p>
          </div>
          <p>ASSET TYPE: <span class="asset-type">{{ content.asset_type }}</span></p>
          </div>
        </div>
      </div> #}
    </div>
{% endif %}